package com.icinfo.creditinfo.bean.analy.cranalyzone;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

import org.apache.commons.lang3.builder.ToStringBuilder;

/** 
 * 描述: 地域分布 实体类<br>
 * @author zhuyong
 * @date 2015-11-06
 * 
 */
public class CrAnalyZone implements Serializable
{

    private static final long	serialVersionUID	= 1L;

    // 
    private BigDecimal id;
    // 地域编码
    private String zoneCode;
    // 企业数量
    private BigDecimal zoneCorpCount;
    // 从业人数统计
    private BigDecimal zoneManCount;
    // 宏观经济指数
    private BigDecimal zoneEcmcIdx;
    // 信贷风险指数
    private BigDecimal zoneRiskIdx;
    // 
    private Date zoneCreateDate;
    //地域名称
    private String zoneName;

    public BigDecimal getId() {
        return id;
    }
    public void setId(BigDecimal id) {
        this.id = id;
    }

    public String getZoneCode() {
        return zoneCode;
    }
    public void setZoneCode(String zoneCode) {
        this.zoneCode = zoneCode;
    }


    public BigDecimal getZoneCorpCount() {
        return zoneCorpCount;
    }
    public void setZoneCorpCount(BigDecimal zoneCorpCount) {
        this.zoneCorpCount = zoneCorpCount;
    }


    public BigDecimal getZoneManCount() {
        return zoneManCount;
    }
    public void setZoneManCount(BigDecimal zoneManCount) {
        this.zoneManCount = zoneManCount;
    }

	public BigDecimal getZoneEcmcIdx() {
        return zoneEcmcIdx;
    }
    public void setZoneEcmcIdx(BigDecimal zoneEcmcIdx) {
        this.zoneEcmcIdx = zoneEcmcIdx;
    }

    public BigDecimal getZoneRiskIdx() {
        return zoneRiskIdx;
    }
    public void setZoneRiskIdx(BigDecimal zoneRiskIdx) {
        this.zoneRiskIdx = zoneRiskIdx;
    }

    public Date getZoneCreateDate() {
        return zoneCreateDate;
    }
    public void setZoneCreateDate(Date zoneCreateDate) {
        this.zoneCreateDate = zoneCreateDate;
    }
	public String getZoneName() {
		return zoneName;
	}
	public void setZoneName(String zoneName) {
		this.zoneName = zoneName;
	}
    
    @Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this);
	}

}
