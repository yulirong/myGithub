package com.icinfo.creditinfo.bean.biz.crwatersewagedischargepay;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

/** 
 * 描述: 污水排放信息表 实体类<br>
 * @author myw
 * @date 2015-11-03
 * 
 */
public class CrWaterSewageDischargePay implements Serializable
{

    private static final long	serialVersionUID	= 1L;

    // 
    private String id;
    // 企业名称
    private String entName;
    // 抄表时间
    private Date meterReadingDate;
    // 污水排放所属期
    private Date sewageDisPeriod;
    // 污水排放量
    private String sewageDisAmount;
    // 交费日期
    private Date payDate;
    // 交费金额
    private String payMoney;
    // 污水排放金额
    private String sewageDisMoney;
    // 
    private String importFrom;
    // 
    private Date importDate;
    // 企业内部序号
    private String corpid;
    
    //辅助字段
    private String analyMeterReadingDateStr;
    private BigDecimal analySewageDisAmountTotal;


    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }


    public String getEntName() {
        return entName;
    }
    public void setEntName(String entName) {
        this.entName = entName;
    }


    public Date getMeterReadingDate() {
        return meterReadingDate;
    }
    public void setMeterReadingDate(Date meterReadingDate) {
        this.meterReadingDate = meterReadingDate;
    }


    public Date getSewageDisPeriod() {
        return sewageDisPeriod;
    }
    public void setSewageDisPeriod(Date sewageDisPeriod) {
        this.sewageDisPeriod = sewageDisPeriod;
    }


    public String getSewageDisAmount() {
        return sewageDisAmount;
    }
    public void setSewageDisAmount(String sewageDisAmount) {
        this.sewageDisAmount = sewageDisAmount;
    }


    public Date getPayDate() {
        return payDate;
    }
    public void setPayDate(Date payDate) {
        this.payDate = payDate;
    }


    public String getPayMoney() {
        return payMoney;
    }
    public void setPayMoney(String payMoney) {
        this.payMoney = payMoney;
    }


    public String getSewageDisMoney() {
        return sewageDisMoney;
    }
    public void setSewageDisMoney(String sewageDisMoney) {
        this.sewageDisMoney = sewageDisMoney;
    }


    public String getImportFrom() {
        return importFrom;
    }
    public void setImportFrom(String importFrom) {
        this.importFrom = importFrom;
    }


    public Date getImportDate() {
        return importDate;
    }
    public void setImportDate(Date importDate) {
        this.importDate = importDate;
    }


    public String getCorpid() {
        return corpid;
    }
    public void setCorpid(String corpid) {
        this.corpid = corpid;
    }
	public String getAnalyMeterReadingDateStr() {
		return analyMeterReadingDateStr;
	}
	public void setAnalyMeterReadingDateStr(String analyMeterReadingDateStr) {
		this.analyMeterReadingDateStr = analyMeterReadingDateStr;
	}
	public BigDecimal getAnalySewageDisAmountTotal() {
		return analySewageDisAmountTotal;
	}
	public void setAnalySewageDisAmountTotal(BigDecimal analySewageDisAmountTotal) {
		this.analySewageDisAmountTotal = analySewageDisAmountTotal;
	}

}
