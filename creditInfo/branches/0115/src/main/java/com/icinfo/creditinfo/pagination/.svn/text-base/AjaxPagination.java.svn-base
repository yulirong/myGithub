package com.icinfo.creditinfo.pagination;

import com.icinfo.plugin.pagination.WebPagination;
import com.icinfo.plugin.pagination.context.PaginationContext;

/** 
 * 描述: TODO<br>
 * @author 夏俊伟
 * @date Apr 17, 2015
 */
public class AjaxPagination extends WebPagination {
	private String dataUrl;
	private String showId;
	public AjaxPagination(Object dataList, Integer totalSize,PaginationContext context) {
		super(dataList, totalSize, context);
	}
	public String getDataUrl() {
		return dataUrl;
	}
	public void setDataUrl(String dataUrl) {
		this.dataUrl = dataUrl;
	}
	public String getShowId() {
		return showId;
	}
	public void setShowId(String showId) {
		this.showId = showId;
	}
}
