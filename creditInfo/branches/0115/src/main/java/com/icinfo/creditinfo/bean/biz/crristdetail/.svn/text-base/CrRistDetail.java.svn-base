package com.icinfo.creditinfo.bean.biz.crristdetail;

import java.io.Serializable;
import java.util.Date;

import org.apache.commons.lang3.builder.ToStringBuilder;

/** 
 * 描述: 风险指数明细 实体类<br>
 * @author gaotong
 * @date 2015-11-03
 * 
 */
public class CrRistDetail implements Serializable
{

    private static final long	serialVersionUID	= 1L;

    // 
    private String id;
    // 
    private String corpid;
    // 指数表关联id
    private String dtIdxId;
    // 指标编码
    private String dtCode;
    // 生成日期
    private Date dtCreateDate;
    // 
    private String dtState;
    //风险类型
    private String name;
    //风险类型数
    private  Integer  count;
   //风险内容
    private String content;
    //风险企业名称
    private String entName;
    
    //预警指标    
    private String idxName;
    //预警级别
    private String idxLevel;
    
    //风险类型编码
    private String idxSupCode;
    
    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }


    public String getCorpid() {
        return corpid;
    }
    public void setCorpid(String corpid) {
        this.corpid = corpid;
    }


    public String getDtIdxId() {
        return dtIdxId;
    }
    public void setDtIdxId(String dtIdxId) {
        this.dtIdxId = dtIdxId;
    }


    public String getDtCode() {
        return dtCode;
    }
    public void setDtCode(String dtCode) {
        this.dtCode = dtCode;
    }


    public Date getDtCreateDate() {
        return dtCreateDate;
    }
    public void setDtCreateDate(Date dtCreateDate) {
        this.dtCreateDate = dtCreateDate;
    }


    public String getDtState() {
        return dtState;
    }
    public void setDtState(String dtState) {
        this.dtState = dtState;
    }
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public Integer getCount() {
		return count;
	}
	public void setCount(Integer count) {
		this.count = count;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getEntName() {
		return entName;
	}
	public void setEntName(String entName) {
		this.entName = entName;
	}
	public String getIdxName() {
		return idxName;
	}
	public void setIdxName(String idxName) {
		this.idxName = idxName;
	}
	public String getIdxLevel() {
		return idxLevel;
	}
	public void setIdxLevel(String idxLevel) {
		this.idxLevel = idxLevel;
	}
	
	public String getIdxSupCode() {
		return idxSupCode;
	}
	public void setIdxSupCode(String idxSupCode) {
		this.idxSupCode = idxSupCode;
	}
	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this);
	}
    

}
