package com.icinfo.creditinfo.bean.upload.tempmortgagesewagedischargeright;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

/** 
 * 描述: 环保排污权抵押 实体类<br>
 * @author gaotong
 * @date 2015-11-12
 * 
 */
public class TempMortgageSewageDischargeRight implements Serializable
{

    private static final long	serialVersionUID	= 1L;

    // 抵押人名称
    private String mortgagorName;
    // 抵押权人名称
    private String mortgageeName;
    // 抵押物名称
    private String guaranty;
    // 抵押废水数量（吨/日）
    private BigDecimal mortgageWastewaterAmount;
    // 抵押二氧化硫数量（吨/年）
    private BigDecimal mortgageSo2Amount;
    // 抵押氮氧化物数量
    private BigDecimal mortgageNoAmount;
    // 抵押物权证编号
    private String guarantyNo;
    // 债务人履行债务期限
    private String debtPayDate;
    // 登记机关
    private String regOrg;
    // 登记时间
    private Date regDate;
    // 
    private String id;
    // 
    private String importFrom;
    // 
    private Date importDate;
    // 企业内部序号
    private String corpid;
    //抵押人组织机构代码
    private String orgCode;
    //抵押人统一信用代码
    private String unscCode;


    public String getMortgagorName() {
        return mortgagorName;
    }
    public void setMortgagorName(String mortgagorName) {
        this.mortgagorName = mortgagorName;
    }


    public String getMortgageeName() {
        return mortgageeName;
    }
    public void setMortgageeName(String mortgageeName) {
        this.mortgageeName = mortgageeName;
    }


    public String getGuaranty() {
        return guaranty;
    }
    public void setGuaranty(String guaranty) {
        this.guaranty = guaranty;
    }


    public BigDecimal getMortgageWastewaterAmount() {
        return mortgageWastewaterAmount;
    }
    public void setMortgageWastewaterAmount(BigDecimal mortgageWastewaterAmount) {
        this.mortgageWastewaterAmount = mortgageWastewaterAmount;
    }


    public BigDecimal getMortgageSo2Amount() {
        return mortgageSo2Amount;
    }
    public void setMortgageSo2Amount(BigDecimal mortgageSo2Amount) {
        this.mortgageSo2Amount = mortgageSo2Amount;
    }


    public BigDecimal getMortgageNoAmount() {
        return mortgageNoAmount;
    }
    public void setMortgageNoAmount(BigDecimal mortgageNoAmount) {
        this.mortgageNoAmount = mortgageNoAmount;
    }


    public String getGuarantyNo() {
        return guarantyNo;
    }
    public void setGuarantyNo(String guarantyNo) {
        this.guarantyNo = guarantyNo;
    }


    public String getDebtPayDate() {
        return debtPayDate;
    }
    public void setDebtPayDate(String debtPayDate) {
        this.debtPayDate = debtPayDate;
    }


    public String getRegOrg() {
        return regOrg;
    }
    public void setRegOrg(String regOrg) {
        this.regOrg = regOrg;
    }


    public Date getRegDate() {
        return regDate;
    }
    public void setRegDate(Date regDate) {
        this.regDate = regDate;
    }


    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }


    public String getImportFrom() {
        return importFrom;
    }
    public void setImportFrom(String importFrom) {
        this.importFrom = importFrom;
    }


    public Date getImportDate() {
        return importDate;
    }
    public void setImportDate(Date importDate) {
        this.importDate = importDate;
    }


    public String getCorpid() {
        return corpid;
    }
    public void setCorpid(String corpid) {
        this.corpid = corpid;
    }
	public String getOrgCode() {
		return orgCode;
	}
	public void setOrgCode(String orgCode) {
		this.orgCode = orgCode;
	}
	public String getUnscCode() {
		return unscCode;
	}
	public void setUnscCode(String unscCode) {
		this.unscCode = unscCode;
	}

}
