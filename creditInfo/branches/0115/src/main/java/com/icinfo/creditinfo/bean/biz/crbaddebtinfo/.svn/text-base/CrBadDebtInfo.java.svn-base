package com.icinfo.creditinfo.bean.biz.crbaddebtinfo;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

import org.apache.struts2.json.annotations.JSON;

/** 
 * 描述: 不良类的债务明细 实体类<br>
 * @author yulirong
 * @date 2015-10-15
 * 
 */
public class CrBadDebtInfo implements Serializable
{

    private static final long	serialVersionUID	= 1L;

    // 
    private String id;
    // 企业名称
    private String badEntName;
    // 注册号
    private String badRegNo;
    // 授信机构
    private String badCreInst;
    // 贷款汇总
    private BigDecimal badLoanSum;
    // 五级分类
    private String badGreadClass;
    // 币种
    private String badCurrency;
    // 借据金额
    private BigDecimal badIouAmount;
    // 放款日期
    private Date badLoanDate;
    // 业务种类
    private String badBusinessType;
    // 担保:1-有，0-无
    private String badGuarantee;
    // 借据余额
    private BigDecimal badIouBalance;
    // 到期日期
    private Date badDueDate;
    // 贷款形式
    private String badLoanForm;
    // 展期:1-有，0-无
    private String badExtension;
    //用户id
    private String badUserId;
    //添加时间
    private Date badOptTime;


    public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }


    public String getBadEntName() {
        return badEntName;
    }
    public void setBadEntName(String badEntName) {
        this.badEntName = badEntName;
    }


    public String getBadRegNo() {
        return badRegNo;
    }
    public void setBadRegNo(String badRegNo) {
        this.badRegNo = badRegNo;
    }


    public String getBadCreInst() {
        return badCreInst;
    }
    public void setBadCreInst(String badCreInst) {
        this.badCreInst = badCreInst;
    }


    public BigDecimal getBadLoanSum() {
        return badLoanSum;
    }
    public void setBadLoanSum(BigDecimal badLoanSum) {
        this.badLoanSum = badLoanSum;
    }


    public String getBadGreadClass() {
        return badGreadClass;
    }
    public void setBadGreadClass(String badGreadClass) {
        this.badGreadClass = badGreadClass;
    }


    public String getBadCurrency() {
        return badCurrency;
    }
    public void setBadCurrency(String badCurrency) {
        this.badCurrency = badCurrency;
    }


    public BigDecimal getBadIouAmount() {
        return badIouAmount;
    }
    public void setBadIouAmount(BigDecimal badIouAmount) {
        this.badIouAmount = badIouAmount;
    }

    @JSON(format = "yyyy/MM/dd")
    public Date getBadLoanDate() {
        return badLoanDate;
    }
    public void setBadLoanDate(Date badLoanDate) {
        this.badLoanDate = badLoanDate;
    }


    public String getBadBusinessType() {
        return badBusinessType;
    }
    public void setBadBusinessType(String badBusinessType) {
        this.badBusinessType = badBusinessType;
    }


    public String getBadGuarantee() {
        return badGuarantee;
    }
    public void setBadGuarantee(String badGuarantee) {
    	this.badGuarantee = badGuarantee;
    }


    public BigDecimal getBadIouBalance() {
        return badIouBalance;
    }
    public void setBadIouBalance(BigDecimal badIouBalance) {
        this.badIouBalance = badIouBalance;
    }

    @JSON(format = "yyyy/MM/dd")
    public Date getBadDueDate() {
        return badDueDate;
    }
    public void setBadDueDate(Date badDueDate) {
        this.badDueDate = badDueDate;
    }


    public String getBadLoanForm() {
        return badLoanForm;
    }
    public void setBadLoanForm(String badLoanForm) {
        this.badLoanForm = badLoanForm;
    }


    public String getBadExtension() {
        return badExtension;
    }
    public void setBadExtension(String badExtension) {
    	this.badExtension = badExtension;
    }
	public String getBadUserId() {
		return badUserId;
	}
	public void setBadUserId(String badUserId) {
		this.badUserId = badUserId;
	}
	public Date getBadOptTime() {
		return badOptTime;
	}
	public void setBadOptTime(Date badOptTime) {
		this.badOptTime = badOptTime;
	}

}
