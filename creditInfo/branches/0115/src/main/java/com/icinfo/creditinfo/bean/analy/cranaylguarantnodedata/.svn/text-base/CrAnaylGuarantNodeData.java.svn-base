package com.icinfo.creditinfo.bean.analy.cranaylguarantnodedata;

import java.io.Serializable;
import java.math.BigDecimal;

/** 
 * 描述: 担保相关指标数据 实体类<br>
 * @author zhuyong
 * @date 2015-10-30
 * 
 */
public class CrAnaylGuarantNodeData implements Serializable
{

    private static final long	serialVersionUID	= 1L;

    // 企业名称
    private String nodeName;
    // 被担保数	
    private BigDecimal nodeGuaranteedNum;
    // 对外担保数
    private BigDecimal nodeExterGuaranteeNum;
    // 总关系数
    private BigDecimal nodeTotalRelation;
    // 被担保金额
    private BigDecimal nodeGuaranteedAmount;
    // 对外担保金额
    private BigDecimal nodeExterGuaranteeAmount;
    // 担保发生额
    private BigDecimal nodeHappenAmount;


    public String getNodeName() {
        return nodeName;
    }
    public void setNodeName(String nodeName) {
        this.nodeName = nodeName;
    }


    public BigDecimal getNodeGuaranteedNum() {
        return nodeGuaranteedNum;
    }
    public void setNodeGuaranteedNum(BigDecimal nodeGuaranteedNum) {
        this.nodeGuaranteedNum = nodeGuaranteedNum;
    }


    public BigDecimal getNodeExterGuaranteeNum() {
        return nodeExterGuaranteeNum;
    }
    public void setNodeExterGuaranteeNum(BigDecimal nodeExterGuaranteeNum) {
        this.nodeExterGuaranteeNum = nodeExterGuaranteeNum;
    }


    public BigDecimal getNodeTotalRelation() {
        return nodeTotalRelation;
    }
    public void setNodeTotalRelation(BigDecimal nodeTotalRelation) {
        this.nodeTotalRelation = nodeTotalRelation;
    }


    public BigDecimal getNodeGuaranteedAmount() {
        return nodeGuaranteedAmount;
    }
    public void setNodeGuaranteedAmount(BigDecimal nodeGuaranteedAmount) {
        this.nodeGuaranteedAmount = nodeGuaranteedAmount;
    }


    public BigDecimal getNodeExterGuaranteeAmount() {
        return nodeExterGuaranteeAmount;
    }
    public void setNodeExterGuaranteeAmount(BigDecimal nodeExterGuaranteeAmount) {
        this.nodeExterGuaranteeAmount = nodeExterGuaranteeAmount;
    }


    public BigDecimal getNodeHappenAmount() {
        return nodeHappenAmount;
    }
    public void setNodeHappenAmount(BigDecimal nodeHappenAmount) {
        this.nodeHappenAmount = nodeHappenAmount;
    }

}
