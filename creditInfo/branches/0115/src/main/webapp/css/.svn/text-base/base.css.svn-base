.page{font-size:14px;color:#666; margin-top:10px;}
.page .selected-item{height:26px;line-height:26px;}
.page .selected-item strong{width:auto;padding-right:17px;}
.page .triangle-down{right:5px;}
.page ul{top:28px;width:96%;}
.page-num{display:inline-block;*display:inline;*zoom:1;width:90%;*padding:4px 0 3px;vertical-align:middle;text-align:right;}
.page-num a, .page-num span{margin:0 5px;padding:3px 0px;}
.page-num label{ padding:8px 5px;}
.page-num .current{background-color:#ddd;border:1px solid #ddd;color:#fff;}
.select-box {
  display: inline-block;
  position: relative;
  z-index: 1;
  margin-right: 5px;
  vertical-align: middle;
}
.select-box .selected-item {
  height: 28px;
  line-height: 28px;
  border: 1px solid #ccc;
}
.selected-item strong {
  display: inline-block;
  overflow: hidden;
  width: 70px;
  height: 26px;
  padding-right: 10px;
  text-indent: 5px;
  font-weight: normal;
  cursor: pointer;
}
.selected-item .triangle-down {
  right: 8px;
}
.triangle-down {
  position: absolute;
  top: 13px;
  right: 0;
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
  border-left: 4px dashed transparent;
  border-right: 4px dashed transparent;
  border-top: 4px solid #666;
  vertical-align: middle;
}
.select-box ul {
  display: none;
  position: absolute;
  top: 30px;
  left: 0;
  width: 97%;
  background-color: #fff;
  border: 1px solid #ccc;
  border-top: none;
}
.select-box li {
  height: 28px;
  line-height: 28px;
  text-indent: 5px;
  cursor: default;
}
.page input {
  width: 60px;
  height: 30px;
  line-height: 18px;
  background-color: #fff;
  border: none;
  text-align:center;
  color: #999;
  vertical-align:middle;
  border:1px solid #e6e6e6;
}
