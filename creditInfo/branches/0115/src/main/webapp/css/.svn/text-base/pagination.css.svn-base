.hx-table {border-left:1px solid #d8d8d8;border-right:1px solid #d8d8d8;}
.hx-table-paging li a {display:block;width:8px;height:11px;}
.hx-table-paging-wrap {float:right;}
.hx-table-paging-home,.hx-table-paging-prev,.hx-table-paging-next,.hx-table-paging-tail{height:11px;display:block;margin-top:4px;width:8px;}
.hx-table-paging .hx-table-paging-home {  background: url(../images/server/biz/page_1.jpg) no-repeat scroll 0 0;}
.hx-table-paging .hx-table-paging-prev {  background: url(../images/server/biz/page_2.jpg) no-repeat scroll 0 0;}
.hx-table-paging .hx-table-paging-next {  background: url(../images/server/biz/page_3.jpg) no-repeat scroll 0 0;}
.hx-table-paging .hx-table-paging-tail {  background: url(../images/server/biz/page_4.jpg) no-repeat scroll 0 0;}
.hx-table-paging-jumpFlag {position:relative;}
.hx-table-paging-jumpFlag .hx-table-paging-go{position:absolute;width:19px;height:19px;right:0;top:0;background: url(../images/server/biz/btn_go.jpg) no-repeat scroll 100% 0;}
.hx-table-paging .hx-table-paging-jump {padding-right:19px;margin-top:0px;width:50px;border:1px solid rgb(228,228,228);height:17px; }
.hx-table-paging {color: #666;margin-top:2px;float:right;height:20px;line-height:20px;}
.hx-table-paging input {color: #999; cursor: pointer;display: block;background:none;border:none;margin-top:4px \9;}
.hx-table-paging .hx-table-paging-hlight { color: #2d7fc0;}
.hx-table-paging .hx-table-paging-hlight:hover {color:#4b98d2;}
.hx-table-paging li {float:left;margin-right:5px;*margin-right:0px;display:inline;}
.hx-table-paging .format span{*margin-top:0px;display:block;float:left;margin-right:2px;margin-left:2px;}
.hx-table-paging .format select {height:21px;float:left;margin-top:2px;}
.hx-table-operation {display:none;}
.hx-table-tip {width:auto;border:1px solid #ccc;background:#F2F2F2;position:absolute;z-index:3;padding:10px;display:none;}
#hx-tablemask {position:absolute;top:0;left:0;bottom:0;width:100%;height:100%;background:#fff;z-index:5;filter:alpha(opacity=60);opacity: 0.6;}

