<struts>

	<package name="crhistoryrecords" extends="icinfo-default" namespace="/crhistoryrecords" >

		<action name="doReadCrHistoryRecordsList" class="crHistoryRecordsAction" method="doReadCrHistoryRecordsListAction">
			<result name="success">/jsp/server/biz/crhistoryrecords/crhistoryrecordsList.jsp</result>
		</action>
		
		<action name="doReadCrHistoryRecordsListJSON" class="crHistoryRecordsAction" method="doReadCrHistoryRecordsListJSONAction">
