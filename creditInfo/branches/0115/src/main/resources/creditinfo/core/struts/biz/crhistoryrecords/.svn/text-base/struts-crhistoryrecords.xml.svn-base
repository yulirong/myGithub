		<action name="doEnAddAndEditCrHistoryRecords" class="crHistoryRecordsAction" method="doEnAddAndEditCrHistoryRecordsAction">
			<result name="success">/jsp/server/biz/crhistoryrecords/crhistoryrecordsAddAndEdit.jsp</result>
