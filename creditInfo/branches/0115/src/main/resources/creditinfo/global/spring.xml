<?xml version="1.0" encoding="UTF-8"?>

<!-- Notice: every spring config file has to include the spring-beans-x.0.xsd 
	properties -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="false">

    <import resource="classpath*:/creditinfo/global/spring-global.xml" />
   <import resource="classpath*:/creditinfo/core/spring/*/spring-*.xml" />
	<import resource="classpath*:/creditinfo/core/spring/*/*/spring-*.xml" />
	<import resource="classpath*:/creditinfo/core/spring/*/*/*/spring-*.xml" />
	<import resource="classpath*:/com/icinfo/plugin/*/xml/spring-*.xml" />
   <!--  <import resource="classpath*:/creditinfo/global/spring-compointcut.xml" />  -->
    <!-- <import resource="classpath*:/creditinfo/global/spring-cxf-context-server.xml" />  -->
	<!-- <import resource="classpath*:/creditinfo/global/spring-datapresto.xml" />--> 
</beans>